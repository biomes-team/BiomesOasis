<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName = "BMT_ChromaticOasis"]/wildPlants</xpath>
					<value>
						<VCE_PeachTree>0.005</VCE_PeachTree>
						<VCE_Opuntia>0.005</VCE_Opuntia>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="BMT_Aquamelon" or defName="BMT_DuneDelight"]</xpath>
					<value>
						<thingCategories>
							<li>VCE_Fruit</li>
						</thingCategories>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

