<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded - More Plants</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="BMT_Lilypad"]/plant |
					Defs/ThingDef[defName="BMT_Papyrus"]/plant |
					Defs/ThingDef[defName="BMT_Cattail"]/plant</xpath>
					<value>
						<sowTags>
							<li>VCE_Aquatic</li>
						</sowTags>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

