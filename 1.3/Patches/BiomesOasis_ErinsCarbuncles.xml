<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Erin's Carbuncles</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BiomeDef[defName="BMT_ChromaticOasis"]/wildAnimals</xpath>
					<value>
						<ERN_Carbuncle>0.03</ERN_Carbuncle>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>