<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="BigBirdThingBase">
		<defName>BMT_PalmWarbler</defName>
		<label>palm warbler</label>
		<description>A small bird commonly found in oases. It is easily spotted for its colorful feathers.</description>
		<statBases>
			<MoveSpeed>4.6</MoveSpeed>
			<MarketValue>80</MarketValue>
			<ComfyTemperatureMin>-10</ComfyTemperatureMin>
			<ComfyTemperatureMax>55</ComfyTemperatureMax>
			<Wildness>0.50</Wildness>
		</statBases>
		<uiIconScale>0.9</uiIconScale>
		<race>
			<herdAnimal>true</herdAnimal>
			<baseBodySize>0.2</baseBodySize>
			<baseHealthScale>0.4</baseHealthScale>
			<baseHungerRate>0.30</baseHungerRate>
			<foodType>VegetarianRoughAnimal</foodType>
			<leatherDef>Leather_Bird</leatherDef>
			<trainability>None</trainability>
			<petness>0.20</petness>
			<nuzzleMtbHours>24</nuzzleMtbHours>
			<lifeExpectancy>7</lifeExpectancy>
			<manhunterOnTameFailChance>0</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0</manhunterOnDamageChance>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
					<soundWounded>Pawn_Chick_Call</soundWounded>
					<soundDeath>Pawn_Chick_Call</soundDeath>
					<soundCall>Pawn_Chick_Call</soundCall>
					<soundAngry>Pawn_Chick_Call</soundAngry>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.15</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.27</minAge>
				</li>
			</lifeStageAges>
		</race>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>BMT_EggPalmWarblerFertilized</eggFertilizedDef>
				<eggUnfertilizedDef>BMT_EggPalmWarblerUnfertilized</eggUnfertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>9</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>1~2</eggCountRange>
			</li>
		</comps>
		<tools>
			<li>
				<label>claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>3.5</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
			</li>
			<li>
				<label>beak</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>4</power>
				<cooldownTime>1.8</cooldownTime>
				<linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<tradeTags>
			<li>AnimalUncommon</li>
		</tradeTags>
	</ThingDef>

